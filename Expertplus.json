      if (!name || !email || !message) {
        status.style.color = 'red';
        status.textContent = '–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –∑–∞–ø–æ–ª–Ω–∏—Ç–µ –æ–±—è–∑–∞—Ç–µ–ª—å–Ω—ã–µ –ø–æ–ª—è: –ò–º—è, Email –∏ –°–æ–æ–±—â–µ–Ω–∏–µ.';
        return;
      }

      // Simple email validation pattern
      const emailPattern = /^[^@\s]+@[^@\s]+\.[^@\s]+$/;
      if (!emailPattern.test(email)) {
        status.style.color = 'red';
        status.textContent = '–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ –∫–æ—Ä—Ä–µ–∫—Ç–Ω—ã–π email.';
        return;
      }

      // Optional phone validation (if filled)
      if (phone) {
        const phonePattern = /^\+?\d{7,15}$/;
        if (!phonePattern.test(phone.replace(/[^\d\+]/g, ''))) {
          status.style.color = 'red';
          status.textContent = '–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ –∫–æ—Ä—Ä–µ–∫—Ç–Ω—ã–π –Ω–æ–º–µ—Ä —Ç–µ–ª–µ—Ñ–æ–Ω–∞.';
          return;
        }
      }

      // Simulate message sending
      status.style.color = '#2980b9';
      status.textContent = '–°–ø–∞—Å–∏–±–æ –∑–∞ –≤–∞—à–µ —Å–æ–æ–±—â–µ–Ω–∏–µ! –ú—ã —Å–≤—è–∂–µ–º—Å—è —Å –≤–∞–º–∏ –≤ –±–ª–∏–∂–∞–π—à–µ–µ –≤—Ä–µ–º—è.';

      this.reset();
    });
  </script>
